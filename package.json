{
  "name": "blog-house",
  "description": "A static site generator for personal blogs based on Sveltekit.",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development svelte-kit dev --host",
    "algolia": "node ./scripts/upload-to-algolia.mjs",
    "process-image": "node ./scripts/process-images.mjs",
    "export": "NODE_ENV=production svelte-kit build",
    "build": "npm-run-all --parallel algolia process-image --serial export"
  },
  "devDependencies": {
    "@giscus/svelte": "^2.2.8",
    "@juggle/resize-observer": "^3.3.1",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-yaml": "^3.1.0",
    "@sveltejs/adapter-static": "1.0.0-next.24",
    "@sveltejs/kit": "1.0.0-next.211",
    "@unocss/preset-typography": "^0.51.8",
    "@unocss/preset-uno": "^0.51.8",
    "@unocss/reset": "^0.51.8",
    "@vercel/analytics": "^1.0.0",
    "algoliasearch": "^4.12.0",
    "carbon-icons-svelte": "^10.44.3",
    "clipboard": "^2.0.8",
    "date-fns": "^2.28.0",
    "debounce": "^1.2.1",
    "dotenv": "^10.0.0",
    "gray-matter": "^4.0.3",
    "html5parser": "^2.0.2",
    "marked": "^2.1.3",
    "npm-run-all": "^4.1.5",
    "object-path": "^0.11.8",
    "parse-html-text-content": "^1.1.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "remove-markdown": "^0.3.0",
    "rollup": "^2.66.0",
    "rollup-plugin-glob": "^1.0.2",
    "rss": "^1.2.2",
    "sharp": "^0.29.3",
    "shiki": "^0.14.2",
    "svelte": "^3.46.2",
    "transliteration": "^2.2.0",
    "unocss": "^0.51.8",
    "vite": "^2.7.13",
    "yaml": "^1.10.2"
  }
}
