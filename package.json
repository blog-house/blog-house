{
  "name": "blog-house",
  "description": "A static site generator for personal blogs based on Sapper.",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development svelte-kit dev",
    "export": "NODE_ENV=production svelte-kit export --entry '/ stub'",
    "watch:tailwind": "postcss static/global.css -o static/styles/global-compiled.css -w",
    "build:tailwind": "NODE_ENV=production postcss static/global.css -o static/styles/global-compiled.css",
    "build": "npm-run-all build:tailwind export"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/runtime": "^7.16.5",
    "@juggle/resize-observer": "^3.3.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-url": "^5.0.1",
    "@rollup/plugin-yaml": "^3.1.0",
    "@sveltejs/adapter-static": "^1.0.0-next.0",
    "@sveltejs/kit": "next",
    "carbon-icons-svelte": "^10.44.3",
    "clipboard": "^2.0.8",
    "date-fns": "^2.27.0",
    "gray-matter": "^4.0.3",
    "html5parser": "^2.0.2",
    "marked": "^2.1.3",
    "npm-run-all": "^4.1.5",
    "object-path": "^0.11.8",
    "parse-html-text-content": "^1.1.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.1",
    "rollup": "^2.61.1",
    "rollup-plugin-glob": "^1.0.2",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "shiki": "^0.9.15",
    "svelte": "^3.44.3",
    "svelte-preprocess": "^4.10.0",
    "svelte-windicss-preprocess": "^4.2.5",
    "transliteration": "^2.2.0",
    "vite": "^2.7.2"
  }
}