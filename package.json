{
  "name": "blog-house",
  "description": "A static site generator for personal blogs based on Sveltekit.",
  "version": "0.3.0",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development svelte-kit dev",
    "postcss": "postcss ./static/styles/global.css -o ./static/styles/global.css",
    "algolia": "node ./scripts/upload-to-algolia.mjs",
    "export": "NODE_ENV=production svelte-kit build",
    "build": "npm-run-all --parallel algolia postcss --serial export",
    "prettier": "prettier --write ./src ./scripts ./shared ./config"
  },
  "devDependencies": {
    "@juggle/resize-observer": "^3.3.1",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-yaml": "^3.1.0",
    "@sveltejs/adapter-static": "^1.0.0-next.23",
    "@sveltejs/kit": "next",
    "algoliasearch": "^4.11.0",
    "carbon-icons-svelte": "^10.44.3",
    "clipboard": "^2.0.8",
    "cssnano": "^5.0.14",
    "date-fns": "^2.27.0",
    "debounce": "^1.2.1",
    "dotenv": "^10.0.0",
    "gray-matter": "^4.0.3",
    "html5parser": "^2.0.2",
    "marked": "^2.1.3",
    "npm-run-all": "^4.1.5",
    "object-path": "^0.11.8",
    "parse-html-text-content": "^1.1.1",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.1",
    "remove-markdown": "^0.3.0",
    "rollup": "^2.62.0",
    "rollup-plugin-glob": "^1.0.2",
    "rollup-plugin-svelte": "^7.1.0",
    "rss": "^1.2.2",
    "shiki": "^0.9.15",
    "svelte": "^3.44.3",
    "svelte-preprocess": "^4.10.1",
    "svelte-windicss-preprocess": "^4.2.5",
    "transliteration": "^2.2.0",
    "vite": "^2.7.8",
    "yaml": "^1.10.2"
  }
}