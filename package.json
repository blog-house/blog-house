{
  "name": "blog-house",
  "description": "A static site generator for personal blogs based on Sveltekit.",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development vite dev --host",
    "algolia": "node ./scripts/upload-to-algolia.mjs",
    "process-image": "node ./scripts/process-images.mjs",
    "export": "NODE_ENV=production vite build",
    "build": "npm-run-all --parallel algolia process-image --serial export",
    "format": "prettier --write src"
  },
  "devDependencies": {
    "@giscus/svelte": "^2.2.8",
    "@juggle/resize-observer": "^3.4.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-yaml": "^3.1.0",
    "@sveltejs/adapter-static": "2.0.3",
    "@sveltejs/kit": "1.30.4",
    "@unocss/preset-typography": "^0.58.5",
    "@unocss/preset-uno": "^0.58.5",
    "@unocss/reset": "^0.58.5",
    "@vercel/analytics": "^1.2.2",
    "@vercel/speed-insights": "^1.0.10",
    "algoliasearch": "^4.22.1",
    "carbon-icons-svelte": "^10.44.3",
    "clipboard": "^2.0.8",
    "date-fns": "^2.28.0",
    "dotenv": "^10.0.0",
    "gray-matter": "^4.0.3",
    "html5parser": "^2.0.2",
    "lodash-es": "^4.17.21",
    "marked": "^2.1.3",
    "npm-run-all": "^4.1.5",
    "object-path": "^0.11.8",
    "overlayscrollbars": "^2.5.0",
    "overlayscrollbars-svelte": "^0.5.3",
    "parse-html-text-content": "^1.1.1",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.1",
    "remove-markdown": "^0.3.0",
    "rollup-plugin-glob": "^1.0.2",
    "rss": "^1.2.2",
    "sharp": "^0.29.3",
    "shiki": "^1.1.6",
    "svelte": "^3.59.2",
    "svelte-action-balancer": "^1.0.4",
    "transliteration": "^2.2.0",
    "unocss": "^0.58.5",
    "vite": "^4.5.2",
    "yaml": "^1.10.2"
  }
}
